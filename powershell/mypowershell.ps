# range operatorを使う
(1..10) | % {Write $_}
# alias
(1..10) | ForEach {Write $_}
(1..10) | ForEach-Object {Write $_}

# base64の文字列をencodeする
Write-Host -NoNewline [System.Convert]::ToBase64String([System.Text.Encoding]::UTF8.GetBytes("TEST!#$%&'()"))

